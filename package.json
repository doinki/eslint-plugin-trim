{
  "name": "eslint-plugin-trim",
  "version": "0.6.0",
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@9.15.4",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "tsx test/argument.test.ts && tsx test/class-name.test.ts",
    "release": "pnpm run build && publint && changeset publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": ">=3",
    "eslint": ">=6"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/parser": {
      "optional": true
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@types/eslint": "^9.6.1",
    "@types/estree": "^1.0.6",
    "eslint": "^9.19.0",
    "prettier": "^3.4.2",
    "publint": "^0.3.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "author": {
    "email": "doinki94@gmail.com",
    "name": "doinki",
    "url": "https://github.com/doinki"
  },
  "bugs": "https://github.com/doinki/eslint-plugin-trim/issues",
  "homepage": "https://github.com/doinki/eslint-plugin-trim#readme",
  "keywords": [
    "eslint",
    "trim",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "git+https://github.com/doinki/eslint-plugin-trim.git"
  }
}
